package CO4;
class auth extends Exception{
	String s;
	auth(String s){
		this.s=s;
	}
	public String toString(){
		return("Exception Msg : "+s);
	}
}
public class pg_3 {
	public static void main(String [] args){
		try{
			throw auth("Newwwwww Ex");
		}
		catch(Exception e){
			System.out.print(e);
		}
		System.out.print("asasa");
	}
}
