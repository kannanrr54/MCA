package CO3;
class person{
	String name,gender,address;
	int age;
	person(int age,String name,String gender,String address){
		this.age=age;
		this.name=name;
		this.gender=gender;
		this.address=address;
	}
}
class employee1 extends person{
	int id;
	String company_name,qualification;
	double salary;
	employee1(int age,String name,String gender,String address,int id,String company_name,String qualification,double salary){
		super(age,name,gender,address);
		this.id=id;
		this.company_name=company_name;
		this.qualification=qualification;
		this.salary=salary;
	}
}
class teacher extends employee{
	int tid;
	String subject,dept; 
	teacher(int id, String name, String address, double salary,int tid,String subject,String dept) {
		super(id, name, address, salary);
		this.subject=subject;
		this.dept=dept;
	}
	
}
