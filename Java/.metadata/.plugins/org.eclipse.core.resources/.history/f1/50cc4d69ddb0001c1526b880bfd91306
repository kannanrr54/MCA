package CO1;
class cpu{
	int price=0;
	class processor{
		int cores;
		String manufacturer;
		processor(int x,String s){
			cores=x;
			manufacturer=s;
		}
		void display(){
			System.out.println("Cores - "+cores+"\n"+"Manufacturer - "+manufacturer);
		}
	}
	static class ram{
		int mem;
		String manufacturer;
		ram(int x,String s){
			mem=x;
			manufacturer=s;
		}
		void display(){
			System.out.println("Memory - "+mem+"\n"+"Ram Manufacturer - "+manufacturer);
		}
	}
}
public class pg_5 {
	public static void main(String [] args){
		cpu c=new cpu();
		System.out.println();
	}
}
